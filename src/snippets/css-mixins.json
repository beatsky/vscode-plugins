{
    ".flex": {
        "prefix": ".flex",
        "body": [
            ".flex();"
        ],
        "description": "[.flex();]"
    },
    ".flex()": {
        "prefix": ".flex",
        "body": [
            ".flex(${1:@justify}, ${2:@align}, ${3:@direction});"
        ],
        "description": "[.flex(@justify: center; @align: center; @direction: row);]"
    },
    ".box-sizing": {
        "prefix": ".box-sizing",
        "body": [
            ".box-sizing(${1:border-box});"
        ],
        "description": "[box-sizing]"
    },
    ".placeholder": {
        "prefix": ".placeholder",
        "body": [
            ".placeholder(${1:@color});"
        ],
        "description": "[placeholder]"
    },
    ".clearfix": {
        "prefix": ".clearfix",
        "body": [
            ".clearfix();"
        ],
        "description": "[.clearfix()]"
    },
    ".size": {
        "prefix": ".size",
        "body": [
            ".size(${1:@width}, ${2:@height});"
        ],
        "description": "[size]"
    },
    ".square": {
        "prefix": ".square",
        "body": [
            ".square(${1:@size});"
        ],
        "description": "[square]"
    },
    ".round": {
        "prefix": ".round",
        "body": [
            ".round(${1:@size});"
        ],
        "description": "[round]"
    },
    ".ellipsis": {
        "prefix": ".ellipsis",
        "body": [
            ".ellipsis(${1:@line-num});"
        ],
        "description": "[超出行数截断只在WebKit内核下有效]"
    },
    ".absolute": {
        "prefix": ".absolute",
        "body": [
            ".absolute(${1:@top}, ${2:@right}, ${3:@bottom}, ${4:@left});"
        ],
        "description": "[例：position: absolute; top: 0; right: 5px; bottom: 10px; left: 15px;]"
    },
    ".fixed": {
        "prefix": ".fixed",
        "body": [
            ".fixed(${1:@top}, ${2:@right}, ${3:@bottom}, ${4:@left});"
        ],
        "description": "[例：position: fixed; top: 0; right: 5px; bottom: 10px; left: 15px;]"
    },
    ".relative": {
        "prefix": ".relative",
        "body": [
            ".relative(${1:@top}, ${2:@right}, ${3:@bottom}, ${4:@left});"
        ],
        "description": "[例：position: relative; top: 0; right: 5px; bottom: 10px; left: 15px;]"
    },
    ".padding": {
        "prefix": ".padding",
        "body": [
            ".padding(${1:@top}, ${2:@right}, ${3:@bottom}, ${4:@left});"
        ],
        "description": "[例：padding-top: 0; padding-right: 5px; padding-bottom: 10px; padding-left: 15px;]"
    },
    ".margin": {
        "prefix": ".margin",
        "body": [
            ".margin(${1:@top}, ${2:@right}, ${3:@bottom}, ${4:@left});"
        ],
        "description": "[例：margin-top: 0; margin-right: 5px; margin-bottom: 10px; margin-left: 15px;]"
    },
    ".border-color": {
        "prefix": ".border-color",
        "body": [
            ".border-color(${1:@color}, ${2:@color}, ${3:@color}, ${4:@color});"
        ],
        "description": "[border-color]"
    },
    ".border-style": {
        "prefix": ".border-style",
        "body": [
            ".border-style(${1:@style}, ${2:@style}, ${3:@style}, ${4:@style});"
        ],
        "description": "[border-style]"
    },
    ".border-width": {
        "prefix": ".border-width",
        "body": [
            ".border-width(${1:@width}, ${2:@width}, ${3:@width}, ${4:@width});"
        ],
        "description": "[border-width]"
    }
}